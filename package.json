{
  "name": "ftth-viability-api",
  "version": "2.0.0",
  "description": "API para consulta de viabilidade FTTH baseada em CTOs",
  "main": "app.js",
  "scripts": {
    "start": "node app.js",
    "dev": "nodemon app.js",
    "test": "jest --testTimeout=10000",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "deploy:dev": "vercel --env NODE_ENV=development",
    "deploy:prod": "vercel --prod --env NODE_ENV=production",
    "logs": "vercel logs"
  },
  "dependencies": {
    "@google-cloud/bigquery": "^7.0.0",
    "cors": "^2.8.5",
    "dotenv": "^16.0.3",
    "express": "^4.22.1",
    "express-rate-limit": "^6.10.0",
    "helmet": "^7.0.0"
  },
  "devDependencies": {
    "eslint": "^8.53.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-plugin-import": "^2.29.0",
    "jest": "^29.7.0",
    "nodemon": "^3.0.1",
    "supertest": "^6.3.3"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "keywords": [
    "ftth",
    "viabilidade",
    "cto",
    "bigquery",
    "api"
  ],
  "author": "Sua Empresa",
  "license": "MIT"
}
